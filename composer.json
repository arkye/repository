{
  "name": "arkye/repository",
  "description": "Arkye Repository",
  "license": "MIT",
  "keywords": [
    "laravel",
    "eloquent",
    "repository",
    "entity",
    "ddd"
  ],
  "support": {
    "issues": "https://github.com/arkye/repository/issues",
    "source": "https://github.com/arkye/repository"
  },
  "authors": [
    {
      "name": "Gustavo Siqueira",
      "email": "gus@brid-it.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "illuminate/database": "^9.0|^10.0",
    "illuminate/support": "^9.0|^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Arkye\\Repository\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": ["tests/helpers.php"]
  },
  "config": {
    "allow-plugins": {
      "phpro/grumphp": false
    }
  }
}
